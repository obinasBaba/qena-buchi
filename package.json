{
  "name": "sms-service",
  "description": "sms service web-app",
  "version": "1.0.0",
  "private": true,
  "author": "rahove thecnology",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix --quiet",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:playground": "nodemon --exec ts-node-esm prisma/seed.ts"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.js"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix --quiet",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "5.12.2",
    "@mui/x-date-pickers": "6.3.0",
    "@tanstack/react-query": "4.29.5",
    "@tanstack/react-query-devtools": "4.29.6",
    "axios": "1.4.0",
    "clsx": "1.2.1",
    "color2k": "2.0.0",
    "date-fns": "2.30.0",
    "formik": "2.2.9",
    "framer-motion": "^10.12.5",
    "gsap": "3.11.3",
    "lodash.debounce": "^4.0.8",
    "next": "^13.3.4",
    "next-auth": "4.22.1",
    "node-fetch": "3.3.1",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "sharp": "0.32.1",
    "simplebar-react": "3.2.3",
    "swiper": "9.2.4",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.9",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "sass": "1.56.1",
    "typescript": "5.0.4"
  }
}
